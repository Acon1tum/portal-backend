# ============================================================================
# DATABASE CONFIGURATION
# ============================================================================

# PostgreSQL Database URL for pgAdmin4
# Format: postgresql://username:password@host:port/database_name
DATABASE_URL="postgresql://postgres:PG_PASSWORD@localhost:5432/maritime_portal"

# Alternative connection format (if needed)
# DATABASE_URL="postgresql://username:password@host:port/database_name?schema=public"

# ============================================================================
# SERVER CONFIGURATION
# ============================================================================

# Server port
PORT=3200

# Session secret for express-session
SESSION_SECRET="your-super-secure-session-secret-key-change-this-in-production"

# ============================================================================
# ENVIRONMENT
# ============================================================================

# Node environment
NODE_ENV="development"

# ============================================================================
# CORS CONFIGURATION
# ============================================================================

# Allowed origins for CORS (comma-separated)
CORS_ORIGIN="http://localhost:3000,http://localhost:5173,http://127.0.0.1:3000"

# ============================================================================
# SECURITY
# ============================================================================

# JWT Secret (if using JWT)
JWT_SECRET="your-jwt-secret-key-change-this-in-production"

# ============================================================================
# LOGGING
# ============================================================================

# Log level
LOG_LEVEL="debug"

# ============================================================================
# POSTGRESQL CONNECTION DETAILS (for reference)
# ============================================================================

# Host: localhost (or your pgAdmin4 server IP)
# Port: 5432 (default PostgreSQL port)
# Database: maritime_portal (or your preferred database name)
# Username: postgres (or your PostgreSQL username)
# Password: your_password (replace with your actual password)

